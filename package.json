{
  "name": "emperor",
  "description": "Information on the Emperors of Rome",
  "author": "bakuzan",
  "private": true,
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/bakuzan/emperor"
  },
  "bugs": {
    "url": "https://github.com/bakuzan/emperor/issues"
  },
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "pretty": "lint-staged",
    "lint": "tslint --fix -c tslint.json 'src/**/*.ts{,x}'",
    "deploy": "yarn build --prefix-paths && gh-pages -d public"
  },
  "lint-staged": {
    "{,src,gatsby}/**/*.{ts,tsx}": [
      "npm run lint",
      "git add"
    ],
    "{,src,gatsby}/**/*.{js}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "ayaka": "file:../ayaka/build",
    "classnames": "^2.2.6",
    "gatsby": "^2.19.12",
    "gatsby-image": "^2.2.40",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-offline": "^3.0.34",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sass": "^2.1.28",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-typescript": "^2.1.27",
    "gatsby-plugin-typography": "^2.3.22",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-json": "^2.2.26",
    "gatsby-transformer-sharp": "^2.3.14",
    "meiko": "file:../meiko/build",
    "nano-css": "^5.2.1",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-fairy-gates": "^0.16.19"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/typography": "^0.16.3",
    "babel-preset-gatsby": "^0.2.28",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "^3.7.5"
  }
}
