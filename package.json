{
  "name": "emperor",
  "description": "Information on the Emperors of Rome",
  "author": "bakuzan",
  "private": true,
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/bakuzan/emperor"
  },
  "bugs": {
    "url": "https://github.com/bakuzan/emperor/issues"
  },
  "scripts": {
    "develop": "gatsby develop",
    "start": "yarn clean && gatsby develop",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "pretty": "lint-staged",
    "lint": "tslint --fix -c tslint.json 'src/**/*.ts{,x}'",
    "deploy": "yarn clean && yarn build --prefix-paths && gh-pages -d public"
  },
  "lint-staged": {
    "{,src,gatsby}/**/*.{ts,tsx}": [
      "npm run lint"
    ],
    "{,src,gatsby}/**/*.{js}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "ayaka": "file:../ayaka/build",
    "classnames": "^2.3.1",
    "gatsby": "^4.8.2",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-manifest": "^4.8.2",
    "gatsby-plugin-offline": "^5.8.2",
    "gatsby-plugin-react-helmet": "^5.8.0",
    "gatsby-plugin-sass": "^5.8.0",
    "gatsby-plugin-sharp": "^4.8.2",
    "gatsby-plugin-typescript": "^4.8.2",
    "gatsby-plugin-typography": "^4.8.0",
    "gatsby-source-filesystem": "^4.8.2",
    "gatsby-transformer-json": "^4.8.0",
    "gatsby-transformer-remark": "^5.8.2",
    "gatsby-transformer-sharp": "^4.8.0",
    "meiko": "file:../meiko/build",
    "nano-css": "^5.3.4",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-typography": "^0.16.20",
    "typography": "^0.16.21",
    "typography-theme-fairy-gates": "^0.16.19"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@types/node": "^13.13.52",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@types/typography": "^0.16.4",
    "babel-preset-gatsby": "^2.8.2",
    "gh-pages": "^2.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.6.2"
  }
}
